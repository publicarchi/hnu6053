         <ref name="att.global.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise le type de section de texte qui apparaîtra par génération automatique (par exemple : index, table des matières, etc.)
Sample values include: 1] index; 2] toc; 3] figlist; 4] tablist</a:documentation>
               <data type="token">
                  <param name="pattern">[^\p{C}\p{Z}]+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
