            <zeroOrMore>
               <ref name="revisionDesc"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="licence"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="model.annotationPart.body"/>
