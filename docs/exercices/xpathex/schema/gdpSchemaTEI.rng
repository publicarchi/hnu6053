               <oneOrMore>
                  <data type="anyURI"/>
               </oneOrMore>
            </list>
         </attribute>
         <optional>
            <attribute name="motivation">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
